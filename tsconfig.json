{
  "compilerOptions": {
      "module": "commonjs",
      "lib": [
          "ES2018"
      ],
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "types": [],
      "noEmit": true,
      "forceConsistentCasingInFileNames": true
  },
  "include": [
    "types",
    "shared",
  ]
}